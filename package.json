{
  "name": "express-async-await",
  "version": "1.0.0",
  "description": "Write Express middleware and route handlers using async/await",
  "main": "dist/index.js",
  "engines": {
    "node": ">= 12.19.0",
    "npm": ">= 6.14.8"
  },
  "scripts": {
    "lint": "eslint src",
    "prettier": "prettier --write **/*.{js,json}",
    "test": "jest --runInBand --projects jest.config.json --forceExit",
    "build": "rimraf dist && babel src --copy-files --out-dir dist",
    "prepublish": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,yml,yml,json}": [
      "prettier --write"
    ]
  },
  "author": "MichalC",
  "license": "MIT License",
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.6",
    "@babel/plugin-syntax-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "express": "^4.17.1",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.0.1"
  }
}
